@startuml

title Application

class Application {
    +USERS ArrayList<User> {static}
    +MOVIES ArrayList<Movie> {static}
}

class Transaction {
    -date: Date
    -user: User
    -movie: Movie
}

class Movie {
    -title: String
    -GENRE: ArrayList<String>
    -year: int
    -price: double
}

@enduml